%h1 Item
%p Serial Number: #{@item.serial_num}
%p Description  : #{@item.description}
%p Supplier 	: #{@item.supplier}
%p Status 		: #{@item.status}
%p Lender 		: #{@item.lender}
%p #{link_to 'Edit', edit_item_path(@item)} | #{link_to 'Destroy', @item, method: :delete, data: {confirm: 'Are you sure?'}} | #{link_to 'Back', items_path}